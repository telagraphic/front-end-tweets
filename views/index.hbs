<h1>Front End Tweeters, Mon</h1>

<ul class="tweets">
  {{#each tweets}}
    <li class="tweet">
      <article class="tweet__article">
        <a href="{{this.link}}" class="tweet__message">
          {{this.message}}
        </a>

        <a href="{{this.site}}" class="tweet__handle">
          &mdash; {{this.handle}}
        </a>
      </article>
    </li>
  {{/each}}
</ul>
