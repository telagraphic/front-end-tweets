<ul class="tweets">
  {{#each tweets}}
    <li class="tweet">
      <time class="tweet__time">{{this.created}}</time>
      <a href="{{this.link}}" class="tweet__message">
        {{this.message}}
      </a>

      <a href="{{this.site}}" class="tweet__handle">
        &mdash; {{this.handle}}
      </a>
    </li>
  {{/each}}
</ul>
